 수 있고, 배열에 넣은 요소에 변화가 있을 때만 해당 코드를 실행시킬 수 있는 방법이다.\n\n\nexport default UseEffect;","/Users/BOM/Desktop/WEB/react-project/react-beginner/src/todolist/Todolist.js",[],[],"/Users/BOM/Desktop/WEB/react-project/react-beginner/src/components/Movie.js",[],[],"/Users/BOM/Desktop/WEB/react-project/react-beginner/src/routes/Home.js",[],[],"/Users/BOM/Desktop/WEB/react-project/react-beginner/src/routes/Detail.js",[],[],"/Users/BOM/Desktop/WEB/react-project/react-beginner/src/components/MovieDetail.js",[],[],{"ruleId":"72","severity":1,"message":"73","line":4,"column":8,"nodeType":"74","messageId":"75","endLine":4,"endColumn":16},{"ruleId":"72","severity":1,"message":"76","line":5,"column":8,"nodeType":"74","messageId":"75","endLine":5,"endColumn":16},{"ruleId":"77","replacedBy":"78"},{"ruleId":"79","replacedBy":"80"},{"ruleId":"77","replacedBy":"81"},{"ruleId":"79","replacedBy":"82"},{"ruleId":"72","severity":1,"message":"83","line":8,"column":9,"nodeType":"74","messageId":"75","endLine":8,"endColumn":21},"no-unused-vars","'Practice' is defined but never used.","Identifier","unusedVar","'ToDoList' is defined but never used.","no-native-reassign",["84"],"no-negated-in-lhs",["85"],["84"],["85"],"'iRunOnlyOnce' is assigned a value but never used.","no-global-assign","no-unsafe-negation"]